<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="0.5dp"
        android:left="0.5dp"
        android:right="0.5dp"
        android:top="0.5dp">
        <selector>
            <item android:state_pressed="true">
                <shape>
                    <solid android:color="#ffffff" />
                    <corners android:radius="0dp" />
                </shape>
            </item>
            <item>
                <shape>
                    <solid android:color="#ffffff" />
                    <corners android:radius="0dp" />
                </shape>
            </item>
        </selector>
    </item>
</layer-list>
